{
    "env": {
        "browser": true,
        "es2021": true,
        "react-native/react-native": true
    },
    "extends": [
        "standard",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-native",
        "react-hooks"
    ],
    "rules": {
        "react/prop-types":0
    },
    "settings": {
        "react": {
          "pragma": "React",
          "version": "18.2.0"
        }
      }
}
